package com.joffice.rcpl.plugin.office.databinding.types;

public class JOPosition {

	private JOEmu x;
	private JOEmu y;

	/**
	 * @param width
	 * @param height
	 */
	public JOPosition(JOEmu x, JOEmu y) {
		this.x = x;
		this.y = y;
	}

	public JOEmu getX() {
		return x;
	}

	public JOEmu getY() {
		return y;
	}

	public void setX(JOEmu x) {
		this.x = x;
	}

	public void setY(JOEmu y) {
		this.y = y;
	}

	public double getXPixel() {
		return x.toPixel();
	}

	public double getYPixel() {
		return y.toPixel();
	}

}
