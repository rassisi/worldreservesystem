package com.joffice.rcpl.plugin.office.databinding.types;

/**
 * @author ramin
 *
 */
public enum JOTextboxType {
	TEXTBOX_SIMPLE_NO_FILL_NO_BORDER("No Fill, no Border", "textbox_simple_no_fill_no_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_NO_FILL_BORDER("No Fill, Border", "textbox_simple_no_fill_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_WHITE_BORDER("White Fill, Border", "textbox_simple_fill_white_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_BLACK_BORDER("Black Fill, Border", "textbox_simple_fill_black_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_1_BORDER("Fill with Accent 1, Border", "textbox_simple_fill_accent_1_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_2_BORDER("Fill with Accent 2, Border", "textbox_simple_fill_accent_2_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_3_BORDER("Fill with Accent 3, Border", "textbox_simple_fill_accent_3_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_4_BORDER("Fill with Accent 4, Border", "textbox_simple_fill_accent_4_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_5_BORDER("Fill with Accent 5, Border", "textbox_simple_fill_accent_5_border"),  //$NON-NLS-2$
	TEXTBOX_SIMPLE_FILL_ACCENT_6_BORDER("Fill with Accent 6, Border", "textbox_simple_fill_accent_6_border")  //$NON-NLS-2$
;
	
	
	private String displayName;
	private String id;
	
	JOTextboxType(String displayName, String id) {
		this.displayName = displayName;
		this.id = id;
	}

	public String getDisplayName() {
		return displayName;
	}

	public String getId() {
		return id;
	}
	
}
