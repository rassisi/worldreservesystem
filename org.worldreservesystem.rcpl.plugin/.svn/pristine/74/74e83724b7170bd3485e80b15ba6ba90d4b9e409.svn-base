package com.joffice.rcpl.plugin.office.databinding.types;

import com.joffice.rcpl.plugin.office.internal.config.JOConversion;

/**
 * @author ramin
 * 
 */
public class JOEmuPixel extends JOEmu {
	public JOEmuPixel(double value) {
		double points = JOConversion.pixelToPoints(value);
		this.value = JOConversion.Pt2Emu(points);
	}

	@Override
	public String toString() {
		return super.toString();
	}
}
