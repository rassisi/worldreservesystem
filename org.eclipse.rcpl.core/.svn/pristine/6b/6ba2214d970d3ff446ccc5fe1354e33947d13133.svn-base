package org.eclipse.rcpl;

public enum EnServiceId {

	BROWSER_SERVICE,

	COLOR_SERVICE,

	COMMAND_SERVICE,

	CONTEXT_MENU_SERVICE,

	DELETE_SERVICE,

	DOCUMENT_SERVICE,

	EDITOR_SERVICE,

	FONT_SERVICE,

	INSERT_SERVICE,

	LAYOUT_SERVICE,

	PARAGRAPH_SERVICE,

	PICTURE_SERVICE,

	SETUP_SERVICE,

	TABLE_SERVICE,

	DEFAULT_SERVICE

	;

	public static EnServiceId getServiceId(String id) {

		if ("standard_and_theme_colors".equals(id)) {
			return COLOR_SERVICE;
		}

		if ("characterStyle".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("homeShowOverview".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("homeShowTutorials".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("homeShowSamples".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("homeShowWhatsNew".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("collapse_all".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("USE_CASE".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("showOutline".equals(id)) {
			return DEFAULT_SERVICE;
		}

		if ("INSERT_PARAGRAPH".equals(id)) {
			return DEFAULT_SERVICE;
		}

		return null;
	}

}
